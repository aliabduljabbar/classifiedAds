<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <display-name>classified</display-name>


    <servlet>
        <display-name>CreateAd</display-name>
        <servlet-name>CreateAd</servlet-name>
        <servlet-class>ad.servlets.CreateAd</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>CreateAd</servlet-name>
        <url-pattern>/ad/CreateAd</url-pattern>
    </servlet-mapping>
</web-app>

<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <display-name>classified</display-name>

    <servlet>
        <display-name>NewAd</display-name>
        <servlet-name>NewAd</servlet-name>
        <servlet-class>ad.servlets.NewAd</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>NewAd</servlet-name>
        <url-pattern>/ad/NewAd</url-pattern>
    </servlet-mapping>

</web-app>
